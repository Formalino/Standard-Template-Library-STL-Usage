#include <iostream>
#include <vector>

int main() {
    std::vector<int> numbers = {1, 2, 3, 4, 5};

    // Accessing elements using iterator
    std::cout << "Elements in the vector: ";
    for (auto it = numbers.begin(); it != numbers.end(); ++it) {
        std::cout << *it << " ";
    }
    std::cout << std::endl;

    // Adding elements
    numbers.push_back(6);
    numbers.push_back(7);

    // Displaying updated elements
    std::cout << "Updated elements in the vector: ";
    for (int num : numbers) {
        std::cout << num << " ";
    }
    std::cout << std::endl;

    return 0;
}
